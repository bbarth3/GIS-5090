   "source": [
    "import arcpy\n",
    "\n",
    "def countstringfields(table):\n",
    "    fields = arcpy.ListFields(table)\n",
    "    n = 0\n",
    "    for field in fields:\n",
    "        if field.type == \"String\":\n",
    "            n += 1\n",
    "\n",
    "    return n"
   ]
